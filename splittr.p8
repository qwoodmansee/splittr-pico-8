blocks = {}
enemies = {}
joiners = {}
score = 0

game_over = false
start_screen = true

--- enemy code --- 
function move_enemies()
	for e in all(enemies) do
		e.y += e.speed
	end
end

function random_gen_splittr()
	if (flr(rnd(100)) < 5) do
		new = {}
		new.x = flr(rnd(120))
		new.y = 0
		new.speed = flr(rnd(2) + 1)
		if (new.speed == 1) do
			new.sprite = 2
		else 
			new.sprite = 3
		end
		
		add(enemies, new)
	end
end

function check_enemy_collisions()
	for i, block in ipairs(blocks) do
		block_edge_right = block.x + block.size
		block_edge_bottom = block.y + block.size
		for e in all(enemies) do
			
		 enemy_bottom_y = e.y + 8
		 enemy_point_left = e.x + 4
		 enemy_point_right = e.x + 5
		 if (enemy_bottom_y == block_edge_bottom) then
			 if (enemy_point_left > block.x and enemy_point_left < block_edge_right) or (enemy_point_right > block.x and enemy_point_right < block_edge_right) then
				 collision = true
				 split_block(i)
			 end
		 end
		end
	end
end

-- joiner code --

function move_joiners()
	for j in all(joiners) do
		j.y += j.speed
	end
end

function random_gen_joiner()
if (flr(rnd(1000)) < 4) do
		new = {}
		new.x = flr(rnd(120))
		new.y = 0
		new.speed = 1
		new.sprite = 7
		
		add(joiners, new)
	end
end

function check_joiner_collisions()
	for i, block in ipairs(blocks) do
		block_edge_right = block.x + block.size
		block_edge_bottom = block.y + block.size
		for ji, j in ipairs(joiners) do
		 joiner_bottom_y = j.y + 8
		 joiner_point_left = j.x + 4
		 joiner_point_right = j.x + 5
		 if (joiner_bottom_y == block_edge_bottom) then
			 if (joiner_point_left > block.x and joiner_point_left < block_edge_right) or (joiner_point_right > block.x and joiner_point_right < block_edge_right) then
				 collision = true
				 join_block(i)
				 del(joiners, joiners[ji])
			 end
		 end
		end
	end
end
--- game logic --- 

function split_block(block_index)
	block = blocks[block_index]

	-- split --
	if (block.size == 8) then
		block.size = 4
		block.speed *= 2
		block.sprite = 5
		if (count(blocks) < 2) then
			sfx(0)
		 add_block(4, block.x + 8)
		end
	else -- destroy --
			sfx(4)
			del(blocks, block)
	end
end

function join_block(block_index)
	
	if (blocks[block_index].size == 8) then 
		score += 1000
		sfx(2) 
	else
		sfx(3)
		block_one = blocks[1]
		block_two = blocks[2]
		if (block_index == 1) then
			block_one.size = 8
			block_one.sprite = 1
			block_one.speed = 2
			del(blocks, blocks[2])
		else
			block_two.size = 8
			block_two.sprite = 1
			block_two.speed = 2
			del(blocks, blocks[1])
		end
	end
end

function add_block(size, x)
	new_block = {}
	new_block.x = x
	new_block.y = 120
	new_block.size = size
	if (size == 8) then
		new_block.sprite = 1
		new_block.speed = 2
	elseif (size == 4) then
		new_block.sprite = 5
		new_block.speed = 4
	else
		new_block.spite = 6
		new_block.speed = 8
	end
	
 add(blocks, new_block)
end

--- input handling --- 

function check_start_game()
 if (btn(0,0) or btn(0,1) or btn(1,0) or btn(1,1)) then
		start_game()
	end
end

function move_players()
	-- s key --
	if (btn(0,1)) then
		moving_block_index = 1
		moving_block = blocks[moving_block_index]
		next_position = moving_block.x - moving_block.speed
		collision_with_block = false
		for i, block in ipairs(blocks) do
			if (i == moving_block_index) then goto continue end
			if (next_position == block.x) then collision_with_block = true end
			::continue::
		end
		
		if (collision_with_block) then
			moving_block.x += 3 * moving_block.speed
			sfx(1)
		else
			moving_block.x -= moving_block.speed
		end
		if (moving_block.x < 0) then
			moving_block.x = 0
		end
	
			-- handle being bounced off screen --
		if (moving_block.x + moving_block.size > 128) then
			moving_block.x = 128 - moving_block.size
		end		
	end
	
	-- f key --
	if (btn(1,1)) then
		moving_block_index = 1
		moving_block = blocks[moving_block_index]
		next_position = moving_block.x + moving_block.speed
		collision_with_block = false
		for i, block in ipairs(blocks) do
			if (i == moving_block_index) then goto continue end
			if (next_position == block.x) then collision_with_block = true end
			::continue::
		end
		
		if (collision_with_block) then
			moving_block.x -= 3 * moving_block.speed
			sfx(1)
		else
			moving_block.x += moving_block.speed
		end
		
		if (moving_block.x + moving_block.size > 128) then
			moving_block.x = 128 - moving_block.size
		end
		
		-- handle being bounced off screen --
		if (moving_block.x < 0) then
			moving_block.x = 0
		end
	end
	
	-- left arrow --
	if (btn(0,0) and count(blocks) > 1) then
		moving_block_index = 2
		moving_block = blocks[moving_block_index]
		next_position = moving_block.x - moving_block.speed
		collision_with_block = false
		for i, block in ipairs(blocks) do
			if (i == moving_block_index) then goto continue end
			if (next_position == block.x) then collision_with_block = true end
			::continue::
		end
		
		if (collision_with_block) then
			moving_block.x += 3 * moving_block.speed
			sfx(1)
		else
			moving_block.x -= moving_block.speed
		end

		if (moving_block.x < 0) then
			moving_block.x = 0
		end
		
		-- handle being bounced off screen --
		if (moving_block.x + moving_block.size > 128) then
			moving_block.x = 128 - moving_block.size
		end
	end
	
	-- right arrow --
	if (btn(1,0) and count(blocks) > 1) then
		moving_block_index = 2
		moving_block = blocks[moving_block_index]
		next_position = moving_block.x + moving_block.speed
		collision_with_block = false
		for i, block in ipairs(blocks) do
			if (i == moving_block_index) then goto continue end
			if (next_position == block.x) then collision_with_block = true end
			::continue::
		end
		
		if (collision_with_block) then
			moving_block.x -= 3 * moving_block.speed
			sfx(1)
		else
			moving_block.x += moving_block.speed
		end

		if (moving_block.x + moving_block.size > 128) then
			moving_block.x = 128 - moving_block.size
		end
		
		-- handle being bounced off screen --
		if (moving_block.x < 0) then
			moving_block.x = 0
		end
	end
end

