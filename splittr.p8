function _update()
	if (start_screen or game_over) then
		check_start_game()
	else
		move_players()
		move_enemies()
		move_joiners()
		check_enemy_collisions()
		check_joiner_collisions()
		random_gen_splittr()
		random_gen_joiner()
		if count(blocks) == 0 then
			game_over = true
		end
	end
end

function _init()
	music(0, 0, 12)
